import java.awt.Color;
import java.awt.EventQueue;
 
import javax.swing.BorderFactory;
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.JTabbedPane;
 
 
public class Overview {
 
	private JFrame frame;
	
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					Overview window = new Overview();
					window.frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}
 
	public Overview() {
		initialize();
	}
 
	private void initialize() {
		
		frame = new JFrame();
		frame.setBounds(100, 100, 520, 505);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.getContentPane().setLayout(null);
		
		JTabbedPane tabbedPane = new JTabbedPane(JTabbedPane.TOP);
		tabbedPane.setBounds(16, 16, 450, 278);
		frame.getContentPane().add(tabbedPane);
		
		JPanel overviewPanel = new JPanel();
		overviewPanel.setLayout(null);
		tabbedPane.addTab("Overview", null, overviewPanel, null);
		
		out(overviewPanel);
		in(overviewPanel);
		missing(overviewPanel);
		overdue(overviewPanel);
 
	}
	
	public static void out(JPanel overviewPanel){
		JPanel outOverview = new JPanel();
		overviewPanel.add(outOverview);
		outOverview.setBorder(BorderFactory.createLineBorder(Color.BLACK, 1));
		outOverview.setBounds(0, 0, 140, 200);
		outOverview.setVisible(true);
	}
	
	public static void in(JPanel overviewPanel){
		JPanel inOverview = new JPanel();
		overviewPanel.add(inOverview);
		inOverview.setBorder(BorderFactory.createLineBorder(Color.BLACK, 1));
		inOverview.setBounds(140, 0, 140, 200);
		inOverview.setVisible(true);
		
	}
	
	public static void missing(JPanel overviewPanel){
		JPanel missingOverview = new JPanel();
		overviewPanel.add(missingOverview);
		missingOverview.setBorder(BorderFactory.createLineBorder(Color.BLACK, 1));
		missingOverview.setBounds(280, 0, 140, 100);
		missingOverview.setVisible(true);
		
	}
	
	public static void overdue(JPanel overviewPanel){
		JPanel overdueOverview = new JPanel();
		overviewPanel.add(overdueOverview);
		overdueOverview.setBorder(BorderFactory.createLineBorder(Color.BLACK, 1));
		overdueOverview.setBounds(280, 100, 140, 100);
		overdueOverview.setVisible(true);
		
		
	}
 
}
